{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off"
  },
  "ignorePatterns": [
    "node_modules/",
    ".next/",
    "out/",
    "public/",
    "*.js",
    "cypress/"
  ]
}